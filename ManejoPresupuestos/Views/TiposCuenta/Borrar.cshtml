@model TipoCuentaViewModel

@{
    ViewData["Title"] = "Borrar Tipo Cuenta";
}

<div class="row">
    <div class="col-md-8  offset-md-2 by-10" id="BorrarTipoDeCuenta">
        <div class="row" id="TituloBorrar">
            <h1>Borrar </h1>
        </div>
        <div class="row" id="SubtituloBorrar">
            <h3 class="mb-3">¡Esta seguro que desea borrar el tipo de cuenta @Model.Nombre</h3>
        </div>
        <div class="row" id="FormularioBorrar">
            <form asp-action="BorrarTipoCuenta">
                <input type="hidden" asp-for="Id" />
                <button type="submit" class="btn" id="BotonBorrar">Borrar</button>
                <a class="btn" asp-action="Index" id="botonCancelarBorar">Cancelar</a>
            </form>
        </div>
    </div>
</div>
